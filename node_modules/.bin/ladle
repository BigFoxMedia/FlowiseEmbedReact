#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/react/lib/cli/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/react/lib/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/react/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/react/lib/cli/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/react/lib/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/react/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules/@ladle/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/@ladle+react@2.5.1_@types+node@18.15.11_react-dom@18.2.0_react@18.2.0_typescript@5.0.3/node_modules:/home/panda/projects/mynerds/flows/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@ladle/react/lib/cli/cli.js" "$@"
else
  exec node  "$basedir/../@ladle/react/lib/cli/cli.js" "$@"
fi
